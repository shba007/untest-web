{
  "name": "untest-web",
  "version": "0.3.3",
  "description": "Learn by quick question answer competition with peers",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/untest-web.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.13",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "db:seed": "prisma db seed",
    "db:backup": "npx tsx prisma/backup.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION_TAG=dev -t untest:dev .",
    "docker:start": "docker run --detach --name untest --env-file .env.prod -p 4201:3000 ghcr.io/shba007/untest-web:latest",
    "tunnel": "untun tunnel --port 4201 http://localhost:4201"
  },
  "prisma": {
    "seed": "jiti prisma/seed.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.4.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.6",
    "@nuxt/test-utils": "^3.19.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^3.0.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.0",
    "@prisma/client": "^6.8.2",
    "@vite-pwa/nuxt": "^1.0.1",
    "@vueuse/core": "^13.2.0",
    "@vueuse/nuxt": "^13.2.0",
    "canvas-confetti": "^1.9.3",
    "confbox": "^0.2.2",
    "eslint": "^9.27.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.17.3",
    "nuxt-icons": "^3.2.1",
    "pinia": "^3.0.2",
    "prisma": "^6.8.2",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^17.4.7",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "playwright-core": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "untun": "^0.1.3",
    "vitest": "^3.1.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "unrs-resolver"
  ]
}
