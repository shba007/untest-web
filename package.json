{
  "name": "untest-web",
  "version": "0.3.1",
  "description": "Learn by quick question answer competition with peers",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shba007/untest-web.git"
  },
  "type": "module",
  "private": true,
  "volta": {
    "node": "22.14.0"
  },
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0"
  },
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "db:seed": "prisma db seed",
    "db:backup": "npx tsx prisma/backup.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION_TAG=dev -t untest:dev .",
    "docker:start": "docker run --detach --name untest --env-file .env.prod -p 4201:3000 ghcr.io/shba007/untest-web:latest",
    "tunnel": "untun tunnel --port 4201 http://localhost:4201"
  },
  "prisma": {
    "seed": "jiti prisma/seed.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.1.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/scripts": "^0.10.4",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^2.2.0",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.10.1",
    "@prisma/client": "^6.4.0",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0",
    "canvas-confetti": "^1.9.3",
    "confbox": "^0.1.8",
    "eslint": "^9.20.1",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.15.4",
    "nuxt-icons": "^3.2.1",
    "pinia": "^3.0.1",
    "prisma": "^6.4.0",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jsonwebtoken": "^9.0.8",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "untun": "^0.1.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}